<Window x:Class="UtilsBox.Views.FileEqual"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UtilsBox.Views"
        mc:Ignorable="d"
        DataContext=""
        Title="FileEqual" Height="450" Width="800">
    <StackPanel>

            <StackPanel>
                <TextBlock x:Name="f1"/>
                <Button Click="OpenFile1" Content="选中第一个文件"/>
            </StackPanel>
            <StackPanel>
                <TextBlock x:Name="f2"/>
            <Button Click="OpenFile2" Content="选中第二个文件"/>
            </StackPanel>


        <Button Click="CompareByMd5" Content="md5比较"/>
        <Button Click="CompareBySha256" Content="sha256比较"/>
        <Button Click="CompareByAllBytes" Content="所有字节比较"/>
    </StackPanel>
</Window>
