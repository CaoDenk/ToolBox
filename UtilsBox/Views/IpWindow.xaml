<Window x:Class="UtilsBox.Views.IpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UtilsBox.Views"
        mc:Ignorable="d"
        Title="IpWindow" Height="450" Width="800">
    <WrapPanel>
        <StackPanel>
            <WrapPanel Margin="5">
                <TextBlock Width="60">ip:</TextBlock>
                <TextBox  Width="200"/>
            </WrapPanel>
            <WrapPanel Margin="5">
                <TextBlock Width="60">mask:</TextBlock>
                <TextBox  Width="200"/>
            </WrapPanel>
        </StackPanel>
        <StackPanel>
            <WrapPanel Margin="5">
                <TextBlock Width="60"><Run Text="ip:"/></TextBlock>
                <TextBox  Width="200"/>
            </WrapPanel>
            <WrapPanel Margin="5">
                <TextBlock Width="60"><Run Text="mask:"/></TextBlock>
                <TextBox  Width="200"/>
            </WrapPanel>
        </StackPanel>
    </WrapPanel>
   
</Window>
